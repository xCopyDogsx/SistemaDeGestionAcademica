<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" href="/img/principal/header.png" />
    @Styles.Render("~/Scripts/css/panel/main.css")
    @Styles.Render("https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css")
</head>
<body class="app sidebar-mini">
    <!-- Navbar-->
    <header class="app-header">
        <a class="app-header__logo" href="index.html">Harrow School</a>
        <!-- Sidebar toggle button--><a class="app-sidebar__toggle" href="#" data-toggle="sidebar" aria-label="Hide Sidebar"></a>
        <!-- Navbar Right Menu-->
        <ul class="app-nav">
            <li class="app-search">
            </li>
            <!-- User Menu-->
            <li class="dropdown">
                <a class="app-nav__item" href="#" data-toggle="dropdown" aria-label="Open Profile Menu"><i class="fa fa-user fa-lg"></i></a>
                <ul class="dropdown-menu settings-menu dropdown-menu-right">
                    <li><a class="dropdown-item" href="page-user.html"><i class="fa fa-cog fa-lg"></i> Settings</a></li>
                    <li><a class="dropdown-item" href="page-user.html"><i class="fa fa-user fa-lg"></i> Profile</a></li>
                    <li><a class="dropdown-item" href="page-login.html"><i class="fa fa-sign-out fa-lg"></i> Logout</a></li>
                </ul>
            </li>
        </ul>
    </header>
    <!-- Sidebar menu-->
    <div class="app-sidebar__overlay" data-toggle="sidebar"></div>
    <aside class="app-sidebar">
        <div class="app-sidebar__user">
            <img class="app-sidebar__user-avatar" src="" alt="User Image">
            <div>
               
                <p class="app-sidebar__user-name">@Session["Nombres"]</p>
                <p class="app-sidebar__user-designation">@Session["Rol"]</p>
            </div>
        </div>
        <ul class="app-menu">
            @if (Session["Rol"].Equals("Administrador"))
            {
                <li><a class="app-menu__item active" href="dashboard.html"><i class="app-menu__icon fa fa-dashboard"></i><span class="app-menu__label">Dashboard</span></a></li>
                <li><a class="app-menu__item" href=""><i class="app-menu__icon fa fa-star"></i><span class="app-menu__label">Ejemplo</span></a></li>
            }
            @if (Session["Rol"].Equals("Estudiante"))
            {


            }
            @if (Session["Rol"].Equals("Acudiente"))
            {


            }
            @if (Session["Rol"].Equals("Docente"))
            {


            }
        </ul>
    </aside>
    @RenderBody()
    @Scripts.Render("~/Scripts/js/panel/jquery-3.3.1.min.js")
    @Scripts.Render("~/Scripts/js/panel/popper.min.js")
    @Scripts.Render("~/Scripts/js/panel/bootstrap.min.js")
    @Scripts.Render("~/Scripts/js/panel/main.js")
    @Scripts.Render("~/Scripts/js/panel/plugins/pace.min.js")
    @RenderSection("scripts", required: false)
</body>
</html>
